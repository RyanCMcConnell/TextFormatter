<site name="vidme" homepage="https://vid.me">
	<example>https://vid.me/rxMHW</example>

	<tags><tag>videos</tag></tags>

	<host>vid.me</host>
	<scrape>
		<extract>#vid\.me/e/(?'id'\w+)#</extract>
		<extract>#oembed.*?url=https://vid\.me/(?!e/)(?'id'\w+)#</extract>
	</scrape>
	<iframe src="https://vid.me/e/{@id}"/>
</site>