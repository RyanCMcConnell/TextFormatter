	<oembed endpoint="https://rutube.ru/api/oembed/"
	          scheme="https://rutube.ru/video/{@id}/"/>

	<tags><tag>.ru</tag></tags>

	<host>rutube.ru</host>
	<extract>!rutube\.ru/(?:play/embed/|tracks/.*?v=|video/)(?'id'\w+)!</extract>
	<iframe src="//rutube.ru/play/embed/{@id}"/>
