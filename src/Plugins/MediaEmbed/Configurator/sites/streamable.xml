<site name="Streamable" homepage="https://streamable.com/">
	<source>https://support.streamable.com/article/46-streamable-api</source>
	<example>https://streamable.com/e4d</example>

	<oembed endpoint="https://api.streamable.com/oembed.json"
	          scheme="https://streamable.com/{@id}"/>

	<tags><tag>videos</tag></tags>

	<host>streamable.com</host>
	<extract>!streamable\.com/(?:e/)?(?'id'\w+)!</extract>
	<iframe src="//streamable.com/e/{@id}"/>
</site>